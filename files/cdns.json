{
    "global": {
        "daemon": true,
        "log": "syslog:daemon",
        "pidfile": "/var/run/cdns.pid",
        "log_debug": false
    },
    "cdns": {
        "listen_ip": "0.0.0.0",
        "listen_port": 5300,
        "timeout": 2,
        "servers": [
            {
                "ip_port": "1.1.1.1"
            }
        ]
    }
}